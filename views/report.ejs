<div class="container" ng-controller="ReportController">
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>

    <!-- Static navbar -->
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Speed Guardian</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="/">Status</a></li>
                    <li><a href="/projects">Projects</a></li>
                    <li class="active"><a href="#">Reports</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
    </nav>

    <h3>Site Performance Report</h3>

    <div class="container-fluid">
        <div class="row button-holder">
            <select class="ddl" ng-options="item.name for item in projects track by item.id" ng-model="selectedProject" ng-change="projectSelected()"></select>
            <select class="ddl" ng-options="item for item in urls" ng-model="selectedUrl" ng-change="urlSelected()"></select>
        </div>
        <div class="row button-holder right-aligned">
            <button type="button" class="btn btn-lg btn-primary" ng-click="regenerate()">Re-Generate</button>
        </div>
    </div>

    <div id="chart_div"></div>

    <script src="/js/report.js"></script>
</div>